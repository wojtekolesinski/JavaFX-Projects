<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.TextFlow?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.client.ClientController">
   <children>
      <TableView fx:id="topicsTable" layoutX="17.0" layoutY="16.0" prefHeight="238.0" prefWidth="113.0" />
      <Button fx:id="saveSubscriptionsButton" layoutX="17.0" layoutY="254.0" mnemonicParsing="false" onAction="#saveSubscriptions" text="Save" />
      <ScrollPane fx:id="articlesMainPane" layoutX="147.0" layoutY="17.0" prefHeight="362.0" prefWidth="431.0">
         <content>
            <TextFlow fx:id="articlesTextFlow" prefHeight="360.0" prefWidth="426.0">
               <padding>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </padding>
            </TextFlow>
         </content>
      </ScrollPane>
      <Button fx:id="refreshButton" layoutX="40.0" layoutY="348.0" mnemonicParsing="false" onAction="#refresh" text="Refresh" />
   </children>
</AnchorPane>
